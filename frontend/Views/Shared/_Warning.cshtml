@if (TempData["info"] != null)
{ 
    <div class='box-alert blue'>

            @TempData["info"]

    </div>
}
@if (TempData["success"] != null)
{ 
    <div class='box-alert green'>

            @TempData["success"]
       
    </div>
}
@if (TempData["warning"] != null)
{ 
    <div class='box-alert yellow'>

            @TempData["warning"]
 
    </div>
}
@if (TempData["error"] != null)
{ 
    <div class='box-alert red'>
      
            @TempData["error"]

    </div>
}
